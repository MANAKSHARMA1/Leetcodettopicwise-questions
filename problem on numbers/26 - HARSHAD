https://takeuforward.org/data-structure/check-if-the-given-number-is-harshador-niven-number/



#include <iostream>
using namespace std;
int main(){
  int n;
  cin>>n;
  int digit=n;
  int sum =0;
  while(digit>0){
      sum += digit % 10;
		digit/= 10;
  }
  if(n % sum==0){
      cout<<"harshad no."<<endl;
  }
  else{
      cout<<"not a harshad no. ";
  }
  
    return 0;
}
