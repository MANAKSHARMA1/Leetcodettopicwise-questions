#include<bits/stdc++.h>
using namespace std;

int main()
 {   
  int n=5;
  int v = 7;
  int arr[]= {2,3,1,7,9};
  //cout<<"enter element "<<endl;
  for(int i=0;i<n;i++){
      if(v==arr[i]){
          cout<<i;
          break;
      }
    //   else{
    //       cout<<"no such element present "<<endl;
    //   }
  }
  
	return 0;
}
