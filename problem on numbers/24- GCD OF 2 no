https://takeuforward.org/data-structure/find-gcd-of-two-numbers/


#include<bits/stdc++.h>
using namespace std;
 
int main(){
    int a,b,gcd;
    cout<<"enter the no.s "<<endl;
    cin>>a>>b;
    for(int i=1;i<=b;i++){
        if(a%i==0 && b%i==0){
             gcd=i;
            // cout<<gcd<<endl;
        }
    }
    cout<<gcd;
 return 0;
}
