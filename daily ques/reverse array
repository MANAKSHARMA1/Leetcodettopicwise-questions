
#include <iostream>

using namespace std;
void print (int ans[],int n){
    for(int i=0;i<n;i++){
        cout<<ans[i]<<" ";
    }
}
void  reverse (int arr[],int n){
int ans[n];
    for(int i=0;i<n;i++){
        ans[n-i-1]=arr[i];
    }
print(ans,n);
}


int main()
{
    int n=5;
    int arr[]={3,6,2,4,9};
    
    reverse (arr,n);

    return 0;
}
