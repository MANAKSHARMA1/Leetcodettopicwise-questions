https://takeuforward.org/data-structure/check-if-a-number-is-prime-or-not/

#include<bits/stdc++.h>
using namespace std;
 

 
int main() {
    int n,check=0;
    cout<<"enter number";
    
    cin>>n;
   for(int i =1; i<=n;i++){
       if(n%i==0){
           check++;
       }
   }
   if(check ==2)
   //only divisible by 1 and number itslef
   {
       cout<<"it is prime ";
   }
   else{
       cout<<"not prime";
   }
   
    return 0;
}
